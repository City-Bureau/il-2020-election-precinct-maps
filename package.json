{
  "name": "il-2020-election-precinct-maps",
  "version": "0.1.0",
  "description": "",
  "private": true,
  "scripts": {
    "format": "prettier --write **/*.{js,css} !dist",
    "start": "NODE_ENV=development run-p -l start:*",
    "start:eleventy": "eleventy --serve",
    "start:rollup": "rollup --config rollup.config.js -w",
    "start:postcss": "postcss ./src/css -d ./site/_includes -w",
    "start:tiles": "node -r esm ./scripts/tile-server.js ./data/precincts/il.mbtiles",
    "start:raster": "http-server -p 8000 ./tiles --cors",
    "build": "NODE_ENV=production run-s -l build:*",
    "build:clean": "rm -rf dist",
    "build:rollup": "rollup --config rollup.config.js",
    "build:postcss": "postcss ./src/css -d ./site/_includes",
    "build:webpack": "webpack --config webpack.config.js",
    "build:eleventy": "eleventy",
    "lint": "eslint ./src/js/**/*.js",
    "style": "prettier --check src/**/**.{js,css}",
    "analyze": "source-map-explorer dist/**/*.js dist/**/*.js.map"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@11ty/eleventy-img": "^0.3.0",
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@mapbox/mbtiles": "^0.12.1",
    "@mapbox/sphericalmercator": "^1.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "canvas": "^2.6.1",
    "core-js": "^3.6.5",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "eslint": "^7.7.0",
    "eslint-plugin-import": "^2.22.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "file-loader": "^6.0.0",
    "html-minifier": "^4.0.0",
    "html-webpack-plugin": "^4.3.0",
    "http-server": "^0.12.3",
    "kdbush": "^3.0.0",
    "mapshaper": "^0.5.26",
    "markdown-it": "^11.0.0",
    "markdown-it-link-attributes": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.32",
    "postcss-cli": "^7.1.1",
    "prettier": "^2.0.5",
    "prettier-eslint": "^11.0.0",
    "rollup": "^2.26.3",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-terser": "^7.0.0",
    "sharp": "^0.26.3",
    "source-map-explorer": "^2.4.2",
    "terser-webpack-plugin": "^4.1.0",
    "tiny-async-pool": "^1.2.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "classlist-polyfill": "^1.2.0",
    "formdata-polyfill": "^3.0.20",
    "mapbox-gl": "^1.12.0"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended"
    ],
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true
    },
    "plugins": [
      "import"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "globals": {
      "mapboxgl": true
    }
  },
  "prettier": {
    "endOfLine": "lf",
    "semi": false,
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "es5"
  }
}
